# ax^2 + bx + c = 0
intrinsic scanf
sta r0				# a
intrinsic scanf
sta r1				# b
intrinsic scanf
sta r2				# c
mulf r1, r1
sta r3				# b^2
lda.immi 4
mulf r0				# 4*a
mulf r1				# (4*a)*b
negf				# -(4*a*b)
addf r3				# D = b^2-4*a*c
